<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="Theme.NewApp" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorAccent</item>
        <item name="colorOnPrimary">@color/white</item>
    </style>
    <style name="Theme.NewApp.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="Theme.NewApp.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="Theme.NewApp.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>
